#include <stdio.h>
#include <stdlib.h>

int main() {
    
	int n, i;
	int count=0;
	printf("Enter the number: ");
	scanf("%d", &n);
	for(i=1; i<n;i++){
		if(n%i==0){
			count++;
		}
	}
	if(count==2){
		printf("The number is prime: %d", n);
	}else{
		printf("The number is composite: %d", n);
	}
    return 0;
}
